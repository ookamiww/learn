QPushButton {
    min-width: 100px;
    max-width: 100px;
    max-height: 100px;
    min-height: 100px;
    border-radius: 50px;
    font-size: 15px;
    color: white;
    background: #f44336 ; /* 默认颜色 */
}

QPushButton:hover {
    font-size: 18px;
    color: white;
    background: #d5d5d5; /* 悬停颜色 */
}

QPushButton:pressed {
    background: #f2f2f2 /* 按下颜色 */
}
